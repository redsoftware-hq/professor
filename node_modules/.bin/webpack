#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/webpack/4.41.6_webpack@4.41.6/node_modules/webpack/bin/webpack.js/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/webpack/4.41.6_webpack@4.41.6/node_modules/webpack/bin/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/webpack/4.41.6_webpack@4.41.6/node_modules/webpack/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/webpack/4.41.6_webpack@4.41.6/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/webpack/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules:/home/ron4ex/projects/redsoftware/common/node_modules:/home/ron4ex/projects/redsoftware/node_modules:/home/ron4ex/projects/node_modules:/home/ron4ex/node_modules:/home/node_modules:/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/webpack/bin/webpack.js/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/webpack/bin/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/webpack/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules:/home/ron4ex/projects/redsoftware/apps/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else 
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi

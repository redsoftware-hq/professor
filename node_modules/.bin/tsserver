#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.3/node_modules/typescript/bin/tsserver/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.3/node_modules/typescript/bin/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.3/node_modules/typescript/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/typescript/3.7.3/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/typescript/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules:/home/ron4ex/projects/redsoftware/common/node_modules:/home/ron4ex/projects/redsoftware/node_modules:/home/ron4ex/projects/node_modules:/home/ron4ex/node_modules:/home/node_modules:/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/typescript/bin/tsserver/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/typescript/bin/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/typescript/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules:/home/ron4ex/projects/redsoftware/apps/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi

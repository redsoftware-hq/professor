#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/9.2.2_2dbcf34c5292cbab6a7f686ad99af238/node_modules/next/dist/bin/next/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/9.2.2_2dbcf34c5292cbab6a7f686ad99af238/node_modules/next/dist/bin/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/9.2.2_2dbcf34c5292cbab6a7f686ad99af238/node_modules/next/dist/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/9.2.2_2dbcf34c5292cbab6a7f686ad99af238/node_modules/next/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/9.2.2_2dbcf34c5292cbab6a7f686ad99af238/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/next/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules/.pnpm/node_modules:/home/ron4ex/projects/redsoftware/common/temp/node_modules:/home/ron4ex/projects/redsoftware/common/node_modules:/home/ron4ex/projects/redsoftware/node_modules:/home/ron4ex/projects/node_modules:/home/ron4ex/node_modules:/home/node_modules:/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/next/dist/bin/next/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/next/dist/bin/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/next/dist/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules/next/node_modules:/home/ron4ex/projects/redsoftware/apps/professor/node_modules:/home/ron4ex/projects/redsoftware/apps/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else 
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
